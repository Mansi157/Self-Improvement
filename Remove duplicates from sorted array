class Solution {
    public int removeDuplicates(int[] nums) {
        if (nums.length == 0) return 0;

        int k = 1;
        for (int i = 1; i < nums.length; i++) {
            if (nums[i] != nums[k - 1]) { // found new unique
                nums[k] = nums[i];
                k++;
            }
        }
        return k;
    }
}

c=1,i=1
003312
step->1 c=0,i=0    skip
step->2 c=1, i=1    
